{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-native",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
  },
  "extends": [
    "@tsconfig/react-native/tsconfig.json",
    "expo/tsconfig.base"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
  ]
}